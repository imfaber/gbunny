{
    "compilerOptions": {
      "outDir": "dist/types",
      "target": "esnext",
      "module": "esnext",
      "moduleResolution": "node",
      "strict": true,
      "declaration": true,
      "noFallthroughCasesInSwitch": true,
      "noImplicitReturns": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "stripInternal": true
    }
  }