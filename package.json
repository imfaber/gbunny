{
  "name": "git-bunny",
  "version": "0.0.1",
  "description": "Enhance interaction with git",
  "license": "MIT",
  "repository": "",
  "main": "dist/index.js",
  "author": {
    "name": "Fabrizio Meinero",
    "email": "fabrizio@imfaber.me",
    "url": "https://imfaber.me"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "git-bunny",
    "git",
    "source control"
  ],
  "bin": {
    "gbunny": "./dist/index.js"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "test": "jest",
    "coverage": "npm test -- --coverage",
    "postcoverage": "open-cli coverage/lcov-report/index.html",
    "lint": "eslint . --ext js,ts,tsx",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "dev": "nodemon --watch src -e ts,js --exec \"npm run build:dev\"",
    "build": "tsc --emitDeclarationOnly && babel src -d dist -x .js,.ts",
    "build:dev": "babel src -d dist -x .js,.ts",
    "prerelease": "npm run lint && npm test && npm run build",
    "release": "standard-version",
    "postpublish": "git push origin master --follow-tags"
  },
  "dependencies": {
    "chalk": "4.0.0",
    "clear": "0.1.0",
    "clui": "0.3.6",
    "figlet": "1.4.0"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/parser": "7.9.4",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/preset-env": "7.9.5",
    "@babel/preset-typescript": "7.9.0",
    "@types/clear": "0.1.0",
    "@types/figlet": "1.2.0",
    "@types/jest": "25.2.1",
    "@types/node": "13.13.4",
    "@typescript-eslint/eslint-plugin": "2.30.0",
    "acorn": "7.1.1",
    "babel-eslint": "10.1.0",
    "babel-jest": "25.4.0",
    "babel-plugin-module-resolver": "4.0.0",
    "bufferutil": "4.0.1",
    "canvas": "2.5.0",
    "configstore": "5.0.1",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-import-resolver-babel-module": "5.1.2",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-prettier": "3.1.3",
    "inquirer": "7.1.0",
    "jest": "25.4.0",
    "minimist": "1.2.5",
    "nodemon": "2.0.3",
    "open-cli": "6.0.1",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "simple-git": "2.1.0",
    "standard-version": "7.1.0",
    "ts-node": "8.9.1",
    "typescript": "3.8.3",
    "utf-8-validate": "5.0.2"
  }
}
